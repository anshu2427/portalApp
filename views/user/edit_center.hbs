<link rel="stylesheet" href="/stylesheets/registration.css" >
  
  <div class="container" style="width:780px"> 
      {{#infoss1 }}
 
<form class="form-control" method="post" enctype="multipart/form-data" action='/user/centerdetailAdmin/edit/{{this._id}}'>

   <div class="form-group border border-info rounded-top clearfix">

     <h2  class="text-center">Center Edit </h2>
<hr class="featurette-divider">


<br>

<div class="card-text">

     <label> Center Name :</label>
   <input class="form-control" type="text" name="centername" value="{{this.centername}}">
   <br>

   <label for="centermobile"> Center Mobile:</label>
  <input class="form-control" type="text" name="centermobile" value="{{this.centermobile}}">
<br>

 <label for="directorname"> Director Name :</label>
<input class="form-control" type="text" name="directorname" value="{{this.directorname}}">
<br>

 <label for="centerphone"> Phone No.  :</label>
<input class="form-control" type="text" name="centerphone" value="{{this.centerphone}}">
<br>

  <label for="centeremail"> Center Email :</label>
 <input class="form-control" type="email" name="centeremail" value="{{this.centeremail}}">
<br>

 <label for="centeraddress">  Center Address :</label>
<input class="form-control" type="text" name="centeraddress" value="{{this.centeraddress}}">
  <br>

 <label for="centerlandmark"> Center Landmark :</label>
<input class="form-control" type="text" name="centerlandmark" value="{{this.centerlandmark}}">

  <hr class="featurette-divider">



<label for="centerimage"><strong>Center Image :&nbsp;&nbsp;&nbsp; </strong> </label>
<input class="form-control" type="file" name="centerimage" value="{{this.centerimage}}" required="centerimage">
<br>
  <a class="btn btn-info" role="button" href='/showcenters' >BACK</a>

 <input class="btn btn-success" type="submit" value="Submit">

</div>


  </div>
  </form>
    {{/infoss1}}


          <hr class="featurette-divider">
