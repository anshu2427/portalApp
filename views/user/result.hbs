
  <link rel="stylesheet" href="/stylesheets/registration.css" >
  
 <div class="container" style="width:700px">  


<form class="form-control" method="post" action="/user/getresults">
   <div class="form-group border border-info rounded-top clearfix">
     <h2  class="text-uppercase text-center">UTEI Examination Results</h2>
         {{#if hasErrors}}
            <div class="alert alert-danger">
                {{# each messages }}
                    <p>{{this}}</p>
                {{/each}}
            </div>
        {{/if}}
<hr class="featurette-divider">
  <label for="rollnumber">Roll Number:</label>
      <input type="number" class="form-control" id="rollnumber" name="rollnumber" placeholder="Enter Roll Number" required="rollnumber">
      <br>
   <label for="dob">DOB:</label>
      <input type="date" class="form-control" id="dob" name="dob" placeholder="Enter DOB" required="dob">
  <br>
   <button type="submit" class="btn btn-primary btn-block">Search</button>
  </div>
  </form>

  
          </div>

          