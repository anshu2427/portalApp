
  <link rel="stylesheet" href="/stylesheets/registration.css" >
    <link rel="stylesheet" href="/stylesheets/BeatPicker.min.css"/>
    <script src="/javascripts/jquery-1.11.0.min.js"></script>
    <script src="/javascripts/BeatPicker.min.js"></script>

 <div class="container" style="width:700px">  


<form class="form-control" method="post" action="/user/getresults">
   <div class="form-group border border-info rounded-top clearfix">
     <h2  class="text-uppercase text-center">UTEI Examination Results</h2>
         {{#if hasErrors}}
            <div class="alert alert-danger">
                {{# each messages }}
                    <p>{{this}}</p>
                {{/each}}
            </div>
        {{/if}}
<hr class="featurette-divider">
  <label for="rollnumber">Roll Number:</label>
      <input type="text" class="form-control" id="rollnumber" name="rollnumber" placeholder="Enter Roll Number" required="rollnumber">  <!--   type change Number to text  -->
      <br>
   <label for="dob">Date of Birth:</label>
    
  <div class="sample">
        <input type="text" 
               id="dob" 
               name="dob"
               placeholder="  DD/MM/YYYY"
               data-beatpicker="true"
               data-beatpicker-position="['right','bottom']"
               data-beatpicker-format="['DD','MM','YYYY'],separator:'/'"
               data-beatpicker-module="footer,clear"
               required="dob"
                /> </div>

  <br>
   <button type="submit" class="btn btn-primary btn-block">Search</button>
  </div>
  </form>

  
          </div>

          