        <main role="main" class="col-sm-9 ml-sm-auto col-md-10 pt-3">
          <h2>Show Upload Result</h2>
 <section class="get">
<table class="table table-bordered table-dark">
  <thead>
    <tr>
      <th scope="col">Student Name</th>
      <th scope="col">Roll Number</th>
      <th scope="col">Course</th>
      <th scope="col">Year</th>
      <th scope="col">DOB</th>
      <th scope="col">DOJ</th>
      <th scope="col">Father's Name</th>
      <th scope="col">Marksheet</th>
      <th scope="col">Certificate</th>
      <th scope="col">Edit/Delete</th>

       </tr>
  </thead>
  <tbody>
       {{# each resulters }}

    <tr>
      <td>{{this.rstudentname}}</td>
       <td>{{this.rrollnumber}}</td>
       <td>{{this.rcourse}}</td>
       <td>{{this.ryear}}</td>
       <td>{{this.rdob}}</td>
       <td>{{this.rdoj}}</td>
       <td>{{this.rfathername}}</td>
       {{#if uploadmarksheet}}
       <td> <img class="first-slide" src="../{{this.uploadmarksheet}}" alt="uploadmarksheet"   width="100" height="51"></td>
       {{else}}
       <td></td>
       {{/if}}
                  {{# if uploadcertificate  }}
       <td> <img class="first-slide" src="../{{this.uploadcertificate}}" alt="uploadcertificate"   width="100" height="51"></td>
       {{else}}
       <td></td>
{{/if}}

        <td><a class="btn btn-primary" role="button" href='/showresults/{{this._id}}'>CLICK</a></td>

    </tr>
      {{/each}}

  </tbody>

</table>  
</section>

        </main>
    

